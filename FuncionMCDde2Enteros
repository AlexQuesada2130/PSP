fun main() {
   	val num1 = 48
	val num2 = 18 
    val resultado = mcd(num1, num2)
    println("El MCD de $num1 y $num2 es $resultado .")
}
//Factorial de 0 o 1 == 1 evitamos posibles errores
fun mcd (a : Int, b : Int) : Int {
    //Realizamos el algoritmo de euclides
    var x = a
    var y = b
    while (y != 0) {
        val res = x % y
        x = y
        y = res
    }
    return x
}
